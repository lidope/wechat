    font-family: SourceHanSansCN-Light;
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: 300;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: SourceHanSansCN-Regular;
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
